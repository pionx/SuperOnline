@model SuperOnline_ASP_NET_MVC.Models.Contenido_Listas

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Contenido_Listas</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.contenido_listas_id)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.contenido_listas_id)
            @Html.ValidationMessageFor(model => model.contenido_listas_id)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.lista_id, "Lista")
        </div>
        <div class="editor-field">
            @Html.DropDownList("lista_id", String.Empty)
            @Html.ValidationMessageFor(model => model.lista_id)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.tipo, "Tipos")
        </div>
        <div class="editor-field">
            @Html.DropDownList("tipo", String.Empty)
            @Html.ValidationMessageFor(model => model.tipo)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
